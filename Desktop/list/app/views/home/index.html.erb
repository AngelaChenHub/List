<div class='container'>
	<h1>Biggs | Chen List of Items</h1>
</div>

<div class='container'>
	<%= form_for @task, :remote => true, :format => :json do |f| %>
	<div class='control-group'>
		<%= f.label :title %>
		<%= f.text_field :title, :class => 'form-control' %>
	</div>

	<%= f.submit 'Add Task', :class => 'btn btn-success' %>
	<% end %>
</div>

<div class="container">
	<h1>To Do List</h1>
<div id="tasks"></div>
</div>